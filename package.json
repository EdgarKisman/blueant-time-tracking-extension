{
  "name": "blueant-time-tracking-extension",
  "private": true,
  "version": "0.0.1",
  "author": "Edgar Kisman",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "pnpm coverage && pnpm format && pnpm lint",
    "test": "pnpx vitest run",
    "coverage": "pnpx vitest run --coverage",
    "ts-lint": "pnpm eslint \"{**/*.ts,**/*.tsx}\"",
    "format": "pnpm exec prettier . --write",
    "lint": "pnpm ts-lint && pnpm css-lint",
    "css-lint": "pnpx stylelint \"**/*.css\""
  },
  "dependencies": {
    "@types/xml2js": "0.4.14",
    "fast-xml-parser": "4.3.2",
    "grommet": "2.35.0-alpha.1",
    "grommet-icons": "4.12.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redaxios": "0.5.1",
    "styled-components": "6.1.8"
  },
  "devDependencies": {
    "@mswjs/interceptors": "0.25.14",
    "@testing-library/react": "14.1.2",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.7",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/coverage-istanbul": "1.1.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "stylelint-config-standard": "36.0.0",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vitest": "1.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
