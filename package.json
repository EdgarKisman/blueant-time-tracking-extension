{
  "name": "blueant-time-tracking-extension",
  "private": true,
  "version": "0.0.1",
  "author": "Edgar Kisman",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "pnpm coverage && pnpm format && pnpm lint",
    "test": "pnpx vitest run",
    "coverage": "pnpx vitest run --coverage",
    "format": "pnpm exec prettier . --write",
    "lint": "pnpm eslint --fix \"{**/*.ts,**/*.tsx}\" && pnpx stylelint \"**/*.css\""
  },
  "dependencies": {
    "@types/xml2js": "0.4.14",
    "eslint-config-prettier": "9.1.0",
    "fast-xml-parser": "4.3.2",
    "grommet": "2.35.0-alpha.1",
    "grommet-icons": "4.11.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redaxios": "0.5.1",
    "styled-components": "6.1.1"
  },
  "devDependencies": {
    "@mswjs/interceptors": "0.25.13",
    "@testing-library/react": "14.1.2",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/coverage-istanbul": "1.0.4",
    "eslint": "8.55.0",
    "eslint-config-standard-with-typescript": "42.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "jsdom": "23.0.1",
    "prettier": "3.1.1",
    "stylelint": "16.0.1",
    "stylelint-config-standard": "35.0.0",
    "typescript": "5.3.3",
    "vite": "5.0.7",
    "vitest": "1.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
